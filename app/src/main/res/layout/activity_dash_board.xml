<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#EDF2F6"
    tools:context=".DashBoard">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:gravity="bottom"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFFFFF"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="ESESPI"
            android:textColor="#000000"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:layout_gravity="bottom"
            app:srcCompat="@drawable/decorative_line" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingVertical="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/DashBoard_btnAcercamientoCiudadano"
                android:layout_width="match_parent"
                android:layout_height="143dp"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="7dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corners_all"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView1er5"
                    android:layout_width="74dp"
                    android:layout_height="74dp"
                    android:paddingLeft="20dp"
                    android:paddingTop="20dp"
                    app:srcCompat="@drawable/dashboard_acercamiento" />

                <TextView
                    android:id="@+id/textView2df8"
                    android:layout_width="181dp"
                    android:layout_height="51dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/poppins_medium"
                    android:paddingLeft="20dp"
                    android:paddingTop="10dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="10dp"
                    android:text="Acercamiento Ciudadano"
                    android:textColor="#000000"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DashBoard_btnDenunciaCiudadana"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_corners_all"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageViewdf15"
                    android:layout_width="69dp"
                    android:layout_height="74dp"
                    android:paddingLeft="22dp"
                    android:paddingTop="17dp"
                    app:srcCompat="@drawable/dashboard_denuncia" />

                <TextView
                    android:id="@+id/textViefw28"
                    android:layout_width="143dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:fontFamily="@font/poppins_medium"
                    android:paddingLeft="20dp"
                    android:paddingTop="10dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="10dp"
                    android:text="Denuncia ciudadana"
                    android:textColor="#000000"
                    android:textSize="14sp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingVertical="7dp">

            <LinearLayout
                android:id="@+id/DashBoard_btnDecomiso"
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:background="@drawable/rounded_corners_all"
                android:orientation="horizontal"
                android:paddingVertical="10dp">

                <ImageView
                    android:id="@+id/imageViedw1sdsd6"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="17dp"
                    app:srcCompat="@drawable/dashboard_decomiso"
                    tools:layout_weight="1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textVfdiew2sdsd9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_medium"
                        android:paddingBottom="2dp"
                        android:text="Decomiso"
                        android:textColor="#000000"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textViefdsdsw30"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:paddingTop="2dp"
                        android:text="Articulos descomisados"
                        android:textSize="14sp" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/DashBoard_btnInfractores"
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:background="@drawable/rounded_corners_all"
                android:orientation="horizontal"
                android:paddingVertical="10dp">

                <ImageView
                    android:id="@+id/imagdfdeView16"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="24dp"
                    app:srcCompat="@drawable/dashboard_infractores"
                    tools:layout_weight="1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textVidfdfew29"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_medium"
                        android:paddingBottom="2dp"
                        android:text="Infractores"
                        android:textColor="#000000"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textVgjfgiew30"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:paddingTop="2dp"
                        android:text="Gestionar infractores"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/DashBoard_btnDetenidos"
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/rounded_corners_all"
                android:orientation="horizontal"
                android:paddingVertical="10dp">

                <ImageView
                    android:id="@+id/imageyjrjView16"
                    android:layout_width="202dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="17dp"
                    app:srcCompat="@drawable/dashboard_detenidos"
                    tools:layout_weight="1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/texdytdhtView29"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_medium"
                        android:paddingBottom="2dp"
                        android:text="Detenidos"
                        android:textColor="#000000"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textVitdhdew30"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins"
                        android:paddingTop="2dp"
                        android:text="Gestionar personas detenidas"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>