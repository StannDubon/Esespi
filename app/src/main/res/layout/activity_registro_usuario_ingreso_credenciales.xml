<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistroUsuarioIngresoCredenciales">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="151dp"
        android:layout_height="77dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066"
        app:srcCompat="@drawable/logo_name_right" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_extrabold"
        android:text="¡Bienvenido!"
        android:textAlignment="center"
        android:textColor="#403F71"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.292"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.241" />

    <EditText
        android:id="@+id/RegistroUsuarioIngresoCredencialestxtUsuario"
        android:layout_width="332dp"
        android:layout_height="51dp"
        android:background="@drawable/rounded_corners_all"
        android:elevation="20dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center_vertical"
        android:hint="Usuario"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:textColor="#000000"
        android:textColorHint="#8C8C8C"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.398" />

    <EditText
        android:id="@+id/RegistroUsuarioIngresoCredencialestxtContraseña"
        android:layout_width="332dp"
        android:layout_height="51dp"
        android:background="@drawable/rounded_corners_all"
        android:elevation="20dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center_vertical"
        android:hint="Contraseña"
        android:inputType="textPersonName"
        android:maxLength="15"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:textColor="#000000"
        android:textColorHint="#8C8C8C"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538" />

    <EditText
        android:id="@+id/RegistroUsuarioIngresoCredencialestxtConfirmacionContraseña"
        android:layout_width="332dp"
        android:layout_height="51dp"
        android:background="@drawable/rounded_corners_all"
        android:elevation="20dp"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center_vertical"
        android:hint="Confirmar contraseña"
        android:inputType="textPersonName"
        android:maxLength="15"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:textColor="#000000"
        android:textColorHint="#8C8C8C"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.691" />

    <Button
        android:id="@+id/RegistroUsuarioIngresoCredencialesBtnRegistrarse"
        android:layout_width="334dp"
        android:layout_height="55dp"
        android:background="@drawable/rounded_corners_all"
        android:backgroundTint="#4399FF"
        android:elevation="20dp"
        android:fontFamily="@font/poppins_semibold"
        android:padding="10dp"
        android:text="Iniciar Sesión"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.532"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.897" />
</androidx.constraintlayout.widget.ConstraintLayout>